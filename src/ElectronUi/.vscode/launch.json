{
		"version": "0.2.0",
		"configurations": [
			{
				"type": "node",
				"request": "launch",
				"name": "Launch Program",
				"program": "${workspaceRoot}\\main.js",
				"stopOnEntry": false,
				"args": [],
				"env": {},
				"sourceMaps": true,
				"outFiles": [
					"${workspaceRoot}/**/*.js"
				],
				"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
				"console": "integratedTerminal",
				"preLaunchTask": "build"
			}
		]
	}