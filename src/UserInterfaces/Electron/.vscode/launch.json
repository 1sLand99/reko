{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/generated/src/main.js",
            "stopOnEntry": false,
            "args": [],
            "env": {},
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/generated/**/*.js"
            ],
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "console": "integratedTerminal",
            "preLaunchTask": "build"
        }
    ]
}